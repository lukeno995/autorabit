                                             <apex:outputText value="0.00" styleClass="form-control" rendered="{!if(childoppobj.PO_ContributoAttivazCollegamScontato__c+childoppobj.PO_Costi_aggiuntivi_collegamento_scontat__c==null,true,false) }"/>
                                        </div>
                                        </div>
                                        
                                        
                                        
                                    </apex:outputPanel> 
                                    
                                    
                                    
                                    
                                    
                                    
                                    
                                    
                                    
                                    
                                </div>
