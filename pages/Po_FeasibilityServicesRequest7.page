                                                    <apex:outputText value="{!childcasedisplayobj.PO_Pop_1_di_riferimento2__c }" styleClass="form-control" />
                                                </div>
                                            </apex:outputPanel>
                                        </div>
                                        
                                        
                                        
                                        
                                    </div>
                                </div>
                                
                                
                                
                            </div>
