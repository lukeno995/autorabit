                                    <div class="col-lg-6">
                                        <label>CreatedDate </label> &nbsp; &nbsp;
                                        <apex:outputField id="CreatedDate" styleClass="form-control" value="{!ChildCase.CreatedDate}"/>
                                    </div>

                                   <div class="col-lg-6">
                                        <label>Nome sito  </label> &nbsp; &nbsp;
                                        <apex:outputField id="Nomesito" styleClass="form-control" value="{!ChildCase.PO_Nome_sito__c}"/>
                                    </div>                                                                                                                                                    
                                </div>
                                
                                <div class="form-group">                                                    
                                    <div class="col-lg-6">
