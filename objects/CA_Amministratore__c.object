    <validationRules>
        <fullName>CA_update_no_amministratore</fullName>
        <active>true</active>
        <description>Controllo sull&apos;aggiornamento dell&apos;amministratore NO_AMMINISTRATORE
Può essere aggiornato solo da amministratori del sistema.</description>
        <errorConditionFormula>AND(
or(
 ISCHANGED(CA_Citt__c), 
 ISCHANGED(CA_Codice_Fiscale__c),  
 ISCHANGED(Name),
 ISCHANGED(CA_External_ID__c),
 ISCHANGED(CA_Email_Amministratore__c),
 ISCHANGED( CA_Indirizzo_Amministratore__c),
 ISCHANGED( CA_Nome_Amministratore__c ), 
 ISCHANGED(CA_P_IVA__c), 
 ISCHANGED( CA_Telefono_Amministratore__c ),
 ISCHANGED(  CA_Albo_Amministratore__c )
), (CA_External_ID__c=&apos;NO_AMMINISTRATORE&apos;)
)</errorConditionFormula>
        <errorMessage>Attenzione!
Non è possibile modificare i dati anagrafici dell&apos;amministratore  &quot;NO_AMMINISTRATORE&quot;.
E&apos; necessario aggiungere un nuovo amministratore.</errorMessage>
    </validationRules>
