<script src="//code.jquery.com/jquery-1.10.2.js"></script>
<script src="//code.jquery.com/ui/1.11.4/jquery-ui.js"></script>
<link rel="stylesheet" type="text/css"
	href="//code.jquery.com/ui/1.10.2/themes/smoothness/jquery-ui.css" />

<style>
.navbar {
	min-height: 105px;
}

.navbar-header {
	float: none;
}

.navbar-default {
	background-color: #ffffff;
}

.panel-default>.panel-heading {
	color: #ffffff;
	background-color: #337ab7;
}

span.dateFormat {
	display: none;
}

.red {
	color: red;
}

.form-check-input {
	margin-left: 45px !important;
	margin-top: 10px;
}

.displayNone {
	display: none;
}

.displayBlock {
	display: block;
}

.ui-autocomplete-loading {
	background: white url(/img/loading32.gif) right center no-repeat;
	background-size: 15px 15px;
}

.placeHolder {
	font-style: italic;
}
</style>




</head>
<body>

			<div id="wrapper">
				<!-- Navigation -->
<nav class="navbar navbar-default navbar-static-top" role="navigation"
	style="margin-bottom: 0">
<c:PO_SideBarComponent Rec="{!recordOfferta}" />

<div class="navbar-default sidebar" role="navigation">
	<div class="sidebar-nav navbar-collapse">
		<ul class="nav" id="side-menu">
		</ul>
	</div>
	<!-- /.sidebar-collapse -->
</div>
<!-- /.navbar-static-side -->
</nav>

        <div id="page-wrapper">
                        
 
 <div class="row">
 <div class="col-lg-8" style="width:73.666667%;">
    
    <div class="panel panel-default">
            <div class="panel-heading">
                <i></i>Dati Desospensione
            </div>
             <div class="panel-body">
                <div class="row">
		<apex:form >
        <div style="color:red;" id="showerror">
            <apex:pageMessages id="errors" /> 
        </div>
        
        <form role="form" class="form-horizontal">
            <fieldset class="form-group">
            	<div class="form-group">
                             
                             
                                  <div class="col-lg-8">
                                   <label>Cronologia Note :</label> &nbsp; &nbsp; <br/>
                                   <apex:outputField id="cronoNote" styleClass="form-control" value="{!sl.EOF_Comunicazioni_con_OLO__c}"/>
                             
                              </div> 
                        
                            
                              <div class="col-lg-4">
                                   <label>Note : </label> &nbsp; &nbsp;
                                   <apex:inputTextarea id="Note" styleClass="form-control" required="true" value="{!sl.EOF_NOTE__c}"/>
                              </div>
                           </div>   
                </fieldset>
        
              	
            <div class="col-sm-6">
                <div class="btn-group" role="group" aria-label="Basic example">                                      
                    <apex:commandButton styleClass="btn btn-primary" style="margin-right:10px;" value="Annulla" action="{!annullaFlusso}" html-formnovalidate="formnovalidate" immediate="true"/>        
                </div>
            </div> 
           	 
                    <div class="col-lg-6" >
                        <div class="btn-group" role="group" aria-label="Basic example">                                      
                            <apex:commandButton styleClass="btn btn-primary" style="margin-right:10px;" value="Inoltra" action="{!inoltraFlusso}" />        
                          </div>
                      </div>

            	</form>
    		</apex:form> 
              </div>
              </div>
              
          
          </div>
      </div>
  </div>
  
	</div>
</div>
<!-- /#wrapper -->

<apex:includeScript value="{!URLFOR($Resource.ENELfiles, 'ENELfiles/EnelStylesheets/files/vendor/jquery/jquery.min.js')}" />

<!-- Bootstrap Core JavaScript -->
			<apex:includeScript value="{!URLFOR($Resource.ENELfiles, 'ENELfiles/EnelStylesheets/files/vendor/bootstrap/js/bootstrap.min.js')}" />
			<script src='https://www.google.com/recaptcha/api.js'></script>

		</body>
	</html>
