                            <div class="col-lg-8" style="width:73.666667%;">
                                
                                <div class="panel panel-default">
                                    <div class="panel-heading">
                                        <i></i>Valori Economici
                                    </div>
                                    
                                    <apex:outputPanel rendered="{!childcasedisplayobj.parent.PO_Offerta__c='P2P Base' ||   childcasedisplayobj.parent.PO_Offerta__c='Evento Temporaneo'}">
                                        <div class="col-lg-6">
                                        <div class="form-group">
                                            <label>Canone Annuo </label> &nbsp; &nbsp;  
                                            <apex:outputText value="{!childoppobj.PO_CanoneCollegamentoScontato__c}" styleClass="form-control" rendered="{!childoppobj.PO_CanoneCollegamentoScontato__c!=null}"/>
