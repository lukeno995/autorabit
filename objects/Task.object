    <validationRules>
        <fullName>CA_3_mesi_per_passare_ad_OF</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
				ISPICKVAL(Status, &apos;Negoziazione Impresa Accesso Censimento Interno KO&apos;),
	   ISPICKVAL(Status, &apos;Negoziazione Impresa Richiesta Permesso Lavori KO&apos;))</errorConditionFormula>
        <errorMessage>Devono essere trascorsi come minimo 3 mesi prima di poter girare la Negoziazione a OpenFiber</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CA_Campo_Note_per_OK</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
				OR(
								ISPICKVAL(Status,&apos;Appuntamento Censimento Interno OK&apos;),
								ISPICKVAL(Status,&apos;Negoziazione Impresa Accesso Censimento Interno OK&apos;),
								ISPICKVAL(Status,&apos;Richiesta Permesso Lavori OK&apos;),
								ISPICKVAL(Status,&apos;Negoziazione Impresa Richiesta Permesso Lavori OK&apos;)),
				 ISBLANK(CA_Note__c ))</errorConditionFormula>
        <errorDisplayField>CA_Note__c</errorDisplayField>
        <errorMessage>Descrivere l&apos;esito positivo del censimento nell&apos;apposito campo Note</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CA_Motivazioni_diniego_obbligat_per_KO</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
ISPICKVAL(CA_Motivazioni_rifiuto__c,&apos;&apos;), 
OR( 
ISPICKVAL(Status, &apos;Appuntamento Censimento Interno KO&apos;), 
ISPICKVAL(Status, &apos;Negoziazione OF Accesso Censimento Interno KO&apos;), 
ISPICKVAL(Status, &apos;Richiesta Permesso Lavori KO&apos;), 
ISPICKVAL(Status, &apos;Negoziazione Impresa Accesso Censimento Interno KO&apos;), 
ISPICKVAL(Status, &apos;Negoziazione Impresa Richiesta Permesso Lavori KO&apos;), 
ISPICKVAL(Status, &apos;Negoziazione OF Richiesta Permesso Lavori KO&apos;)))</errorConditionFormula>
        <errorDisplayField>CA_Motivazioni_rifiuto__c</errorDisplayField>
        <errorMessage>E&apos; necessario specificare una Motivazione Rifiuto</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CA_Motivazioni_rifiuto_altro</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
ISPICKVAL(CA_Motivazioni_rifiuto__c ,&apos;Altro&apos;), 
ISBLANK(CA_Motivazione_KO__c ))</errorConditionFormula>
        <errorDisplayField>CA_Motivazione_KO__c</errorDisplayField>
        <errorMessage>Descrivere la motivazione rifiuto nell&apos;apposito campo</errorMessage>
    </validationRules>
