    <validationRules>
        <fullName>SN_No_edit_case_if_not_owner</fullName>
        <active>false</active>
        <errorConditionFormula>AND(  
SN_Case_Owner__c &lt;&gt;CASESAFEID(&apos;00558000002cJj4&apos;), 
SN_Case_Owner__c &lt;&gt;CASESAFEID($User.Id), 
NOT(ISCHANGED(SN_Case_Owner__c)))</errorConditionFormula>
        <errorMessage>Non è possibile modificare il Social Post se il relativo Caso non è assegnato alla propria utenza.</errorMessage>
    </validationRules>
