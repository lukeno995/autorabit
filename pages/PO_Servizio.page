                                     <div class="col-lg-6">
                                        <label>Collegamento  </label> &nbsp; &nbsp;
                                        <apex:outputField id="Collegamento" styleClass="form-control" value="{!displayCase.CaseNumber}"/>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="col-lg-6">
                                        <label>Area Geografica  </label> &nbsp; &nbsp;
                                        <apex:outputField id="AreaGeografica" styleClass="form-control"  value="{!ChildCase.PO_Area_Geografica__c}"/>
                                    </div>
                                    <div class="col-lg-6">
                                        <label>Siti da collegare  </label> &nbsp; &nbsp;
                                        <apex:outputField id="Sitidacollegare" styleClass="form-control" value="{!ChildCase.PO_Siti_da_collegare__c}"/>
