                                        </div>
                                        </div>
                                    </apex:outputPanel>     
                                    
                                    <apex:outputPanel rendered="{!childcasedisplayobj.parent.PO_Offerta__c='IRU'}">
                                          <div class="col-lg-6">
                                        <div class="form-group">
                                            <label>Canone Manutenzione Annuo  </label> &nbsp; &nbsp;
                                            <apex:outputText value="{!childoppobj.PO_CanoneManutenzioneCollegScontato__c}" styleClass="form-control" rendered="{!childoppobj.PO_CanoneManutenzioneCollegScontato__c!=null}"/>
