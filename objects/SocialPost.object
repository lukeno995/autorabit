        <fullName>SN_Case_Owner__c</fullName>
        <externalId>false</externalId>
        <formula>IF(NOT(ISBLANK(Parent.Owner:Queue.QueueName)), Parent.Owner:Queue.QueueName,  Parent.Owner:User.FirstName &amp;&apos; &apos; &amp; Parent.Owner:User.LastName)</formula>
        <label>Case Owner</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>SN_Case_Status__c</fullName>
        <externalId>false</externalId>
        <formula>TEXT(Parent.Status)</formula>
        <label>Case Status</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>SN_CheckOwner__c</fullName>
        <externalId>false</externalId>
        <formula>IF(Parent.Owner:User.Id = $User.Id , 1, 0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>CheckOwner</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
